{
  "name": "babel-plugin-direct-import",
  "version": "0.6.3",
  "description": "Babel plugin to cherry-pick ES module imports",
  "keywords": [
    "babel-plugin",
    "modules",
    "materia-ui",
    "luxon"
  ],
  "repository": "https://github.com/umidbekk/babel-plugin-direct-import.git",
  "license": "MIT",
  "author": "Umidbek Karimov <uma.karimov@gmail.com>",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc --build tsconfig.build.json",
    "release": "np --no-2fa",
    "tdd": "jest --watch",
    "pretest": "tsc && js-tools lint && yarn build",
    "test": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "tsc"
    }
  },
  "lint-staged": {
    "*": [
      "js-tools lint --fix"
    ]
  },
  "prettier": "@superdispatch/prettier-config",
  "jest": {
    "coverageProvider": "v8",
    "globals": {
      "ts-jest": {
        "isolatedModules": true,
        "tsconfig": "tsconfig.test.json"
      }
    },
    "preset": "ts-jest",
    "testEnvironment": "node",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "dependencies": {
    "resolve": "^1.15.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/parser": "^7.12.15",
    "@babel/plugin-syntax-flow": "^7.12.13",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@superdispatch/eslint-plugin": "^0.3.0",
    "@superdispatch/js-tools": "^0.3.0",
    "@superdispatch/prettier-config": "^0.3.0",
    "@superdispatch/tsconfig": "^0.3.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.14.25",
    "@types/resolve": "^1.19.0",
    "eslint": "^7.19.0",
    "husky": "^4.3.8",
    "jest": "^26.5.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.3",
    "luxon": "^1.23.0",
    "np": "^7.2.0",
    "prettier": "^2.0.2",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@babel/core": "^7.8.0"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
