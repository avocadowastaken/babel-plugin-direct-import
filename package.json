{
  "name": "babel-plugin-direct-import",
  "version": "0.9.2",
  "description": "Babel plugin to cherry-pick ES module imports",
  "keywords": [
    "babel-plugin",
    "modules",
    "materia-ui",
    "luxon"
  ],
  "repository": "https://github.com/umidbekk/babel-plugin-direct-import.git",
  "license": "MIT",
  "author": "Umidbek Karimov <4734297+umidbekk@users.noreply.github.com>",
  "main": "lib/plugin.js",
  "files": [
    "lib",
    "!*.spec.js"
  ],
  "scripts": {
    "lint": "prettier . --check",
    "prepare": "husky install",
    "prerelease": "npm i && npm ddp && npm run lint",
    "release": "np --no-2fa",
    "tdd": "jest --watch",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*": "prettier --write",
    "package-lock.json": "npm ddp"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/test/",
      "/node_modules/"
    ],
    "testEnvironment": "node",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "dependencies": {
    "resolve": "^1.20.0"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/parser": "7.15.3",
    "@babel/plugin-syntax-flow": "7.14.5",
    "@material-ui/core": "5.0.0-beta.4",
    "@material-ui/icons": "5.0.0-beta.4",
    "@material-ui/lab": "5.0.0-alpha.43",
    "@types/jest": "27.0.1",
    "@types/node": "16.6.1",
    "@types/resolve": "1.20.1",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "jest-watch-typeahead": "0.6.4",
    "lint-staged": "11.1.2",
    "luxon": "2.0.2",
    "np": "7.5.0",
    "prettier": "2.3.2",
    "prettier-plugin-packagejson": "2.2.11",
    "typescript": "4.3.5",
    "yup": "0.32.9"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
