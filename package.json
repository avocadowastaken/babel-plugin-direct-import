{
  "name": "babel-plugin-direct-import",
  "version": "0.8.4",
  "description": "Babel plugin to cherry-pick ES module imports",
  "keywords": [
    "babel-plugin",
    "modules",
    "materia-ui",
    "luxon"
  ],
  "repository": "https://github.com/umidbekk/babel-plugin-direct-import.git",
  "license": "MIT",
  "author": "Umidbek Karimov <4734297+umidbekk@users.noreply.github.com>",
  "main": "lib/plugin.js",
  "files": [
    "lib",
    "!*.spec.js"
  ],
  "scripts": {
    "prepare": "husky install",
    "release": "np --no-2fa",
    "tdd": "jest --watch",
    "pretest": "tsc && js-tools lint",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*": [
      "js-tools lint --fix"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/test/",
      "/node_modules/"
    ],
    "testEnvironment": "node",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "dependencies": {
    "resolve": "^1.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/parser": "^7.14.7",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@superdispatch/eslint-plugin": "^0.4.1",
    "@superdispatch/js-tools": "^0.4.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.2",
    "@types/resolve": "^1.20.1",
    "eslint": "^7.30.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.0.1",
    "luxon": "^2.0.1",
    "np": "^7.5.0",
    "prettier": "^2.3.2",
    "prettier-plugin-packagejson": "^2.2.11",
    "typescript": "^4.3.5",
    "yup": "^0.32.9"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
