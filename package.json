{
  "name": "babel-plugin-direct-import",
  "version": "0.5.0",
  "description": "Babel plugin to cherry pick imports of es6 modules.",
  "repository": "https://github.com/umidbekkarimov/babel-plugin-direct-import.git",
  "license": "MIT",
  "author": "Umidbek Karimov <uma.karimov@gmail.com>",
  "main": "src/index.js",
  "scripts": {
    "build": "pika build",
    "prebump": "tsc && yarn lint",
    "bump": "pika publish --no-publish",
    "lint": "js-tools lint",
    "tdd": "jest --watch",
    "test": "jest --coverage",
    "version": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "tsc"
    }
  },
  "lint-staged": {
    "*": [
      "js-tools lint --fix"
    ]
  },
  "prettier": "@superdispatch/prettier-config",
  "dependencies": {
    "resolve": "^1.15.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/parser": "^7.8.0",
    "@babel/plugin-syntax-flow": "^7.8.3",
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@pika/cli": "^0.2.0",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-standard-pkg": "^0.9.2",
    "@superdispatch/babel-preset": "^0.1.51",
    "@superdispatch/eslint-plugin": "^0.1.50",
    "@superdispatch/js-tools": "^0.1.50",
    "@superdispatch/prettier-config": "^0.1.50",
    "@superdispatch/tsconfig": "^0.1.50",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.11.1",
    "@types/resolve": "^1.14.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "jest": "^25.2.3",
    "lint-staged": "^10.1.3",
    "luxon": "^1.23.0",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "@babel/core": "^7.8.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*",
            "__testutils__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  }
}
