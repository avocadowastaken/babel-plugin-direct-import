{
  "name": "babel-plugin-direct-import",
  "version": "1.0.0",
  "description": "Babel plugin to cherry-pick ES module imports",
  "keywords": [
    "babel-plugin",
    "modules",
    "materia-ui"
  ],
  "repository": "https://github.com/umidbekk/babel-plugin-direct-import.git",
  "license": "MIT",
  "author": "Umidbek Karimov <4734297+umidbekk@users.noreply.github.com>",
  "main": "lib/plugin.js",
  "files": [
    "lib",
    "!*.spec.js"
  ],
  "scripts": {
    "checks": "npx tsc && npm run lint && npm run format -- --no-write && npm t",
    "format": "prettier --write --check .",
    "lint": "eslint .",
    "prepare": "husky install",
    "release": "npx --yes np --test-script checks",
    "tdd": "jest --watch",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.*": "prettier --write --ignore-unknown"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2022
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended-script"
    ],
    "overrides": [
      {
        "files": [
          "test/**",
          "*.spec.js"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/test/",
      "/node_modules/"
    ],
    "snapshotSerializers": [
      "./test/pathSerializer.js"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      ".+"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "dependencies": {
    "resolve": "^1.22.1"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/parser": "7.21.2",
    "@babel/plugin-syntax-flow": "7.18.6",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.3",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@mui/core": "5.0.0-alpha.54",
    "@mui/icons-material": "5.11.11",
    "@mui/lab": "5.0.0-alpha.121",
    "@mui/material": "5.11.11",
    "@mui/system": "5.11.11",
    "@types/jest": "29.4.0",
    "@types/node": "18.14.4",
    "@types/resolve": "1.20.2",
    "@umidbekk/configs": "0.2.0",
    "eslint": "8.35.0",
    "eslint-plugin-node": "11.1.0",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "jest-watch-typeahead": "2.2.2",
    "lint-staged": "13.1.2",
    "prettier": "2.8.4",
    "prettier-plugin-jsdoc": "0.4.2",
    "prettier-plugin-packagejson": "2.4.3",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "volta": {
    "node": "14.21.3",
    "npm": "7.24.2"
  }
}
