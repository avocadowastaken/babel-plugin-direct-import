{
  "name": "babel-plugin-direct-import",
  "version": "0.6.4",
  "description": "Babel plugin to cherry-pick ES module imports",
  "keywords": [
    "babel-plugin",
    "modules",
    "materia-ui",
    "luxon"
  ],
  "repository": "https://github.com/umidbekk/babel-plugin-direct-import.git",
  "license": "MIT",
  "author": "Umidbek Karimov <4734297+umidbekk@users.noreply.github.com>",
  "main": "lib/plugin.js",
  "files": [
    "lib",
    "!*.spec.js"
  ],
  "scripts": {
    "prepare": "husky install",
    "release": "np --no-2fa",
    "tdd": "jest --watch",
    "pretest": "tsc && js-tools lint",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*": [
      "js-tools lint --fix"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/integration/",
      "/node_modules/"
    ],
    "coverageProvider": "v8",
    "testEnvironment": "node",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "dependencies": {
    "resolve": "^1.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/parser": "^7.14.1",
    "@babel/plugin-syntax-flow": "^7.12.13",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@superdispatch/eslint-plugin": "^0.3.0",
    "@superdispatch/js-tools": "^0.3.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "@types/resolve": "^1.20.0",
    "eslint": "^7.25.0",
    "husky": "^6.0.0",
    "jest": "^26.5.3",
    "jest-watch-typeahead": "^0.6.3",
    "lint-staged": "^10.5.4",
    "luxon": "^1.26.0",
    "np": "^7.5.0",
    "prettier": "^2.0.2",
    "prettier-plugin-jsdoc": "^0.3.22",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "@babel/core": "^7.8.0"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
