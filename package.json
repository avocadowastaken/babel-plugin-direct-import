    "babel-core": "^6.26.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "codecov": "^3.0.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-node": "^8.0.1",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.2",
    "jest": "^23.0.0",
    "prettier": "^1.16.1",
    "stylotron": "^0.0.13"
  }
