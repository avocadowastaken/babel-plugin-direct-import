    "cover": "jest --coverage --no-cache",
    "codecov": "codecov -f ./coverage/lcov.info"
  },
  "dependencies": {
    "babylon": "^6.18.0"
  },
  "devDependencies": {
    "@dc0de/eslint-config-base": "^0.5.1",
