{
  "name": "babel-plugin-direct-import",
  "version": "0.5.0",
  "description": "Babel plugin to cherry pick imports of es6 modules.",
  "main": "src/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": "https://github.com/umidbekkarimov/babel-plugin-direct-import.git",
  "author": "Umidbek Karimov <uma.karimov@gmail.com>",
  "license": "MIT",
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.spec.js"
    ]
  },
  "scripts": {
    "all": "run-s format lint cover",
    "test": "jest",
    "cover": "jest --coverage --no-cache",
    "codecov": "codecov -f ./coverage/lcov.info",
    "lint": "eslint \"{src,tools}/**/*.js\"",
    "format": "prettier --write \"{README.md,{src,tools}/**/*.js}\"",
    "report": "opener ./coverage/lcov-report/index.html"
  },
  "dependencies": {
    "babylon": "^6.18.0",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "codecov": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "fs-extra": "^4.0.1",
    "glob": "^7.1.2",
    "jest": "^21.2.1",
    "npm-run-all": "^4.1.2",
    "opener": "^1.4.3",
    "prettier": "^1.8.2",
    "rimraf": "^2.6.1"
  }
}
