{
  "root": true,
  "overrides": [
    {
      "files": "*.js",
      "extends": "plugin:@superdispatch/node",
      "rules": {
        "quotes": "off"
      }
    },

    {
      "files": ["*.spec.js", "test/**/*.js"],
      "extends": "plugin:@superdispatch/jest",
      "rules": {
        "quotes": "off"
      }
    }
  ]
}
